@using ECommerce.Models
@model List<Order>
<table>
    <tr>
        <th>Customer Name</th>
        <th>Product</th>
        <th>Quantity</th>
        <th>Date</th>
    </tr>
    @foreach (var order in Model) {
        <tr>
            <td>@order.customer.name</td>
            <td>@order.product.name</td>
            <td>@order.quantity</td>
            <td>@order.created_at.ToString("MMMM d, yyyy")</td>
        </tr>
    }
</table>